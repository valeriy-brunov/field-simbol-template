{
    "name": "valeriy-brunov/field-simbol-template",
    "description": "Помогает пользователю вводить текст по шаблону в текстовом поле. По умолчанию установлен шаблон ввода номера сотового телефона.",
    "type": "cakephp-plugin",
    "home": "https://github.com/valeriy-brunov/field-simbol-template",
    "license": "MIT",
    "require": {
        "php": ">=7.2",
        "cakephp/cakephp": "4.4.*",
        "valeriy-brunov/web-component": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5 || ^9.3"
    },
    "autoload": {
        "psr-4": {
            "Field-simbol-template\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Field-simbol-template\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    }
}
